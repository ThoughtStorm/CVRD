<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Due_date</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
NOT(ISBLANK(MUSW__Due_Date__c)),
TODAY() &gt; MUSW__Due_Date__c
)</errorConditionFormula>
    <errorDisplayField>MUSW__Due_Date__c</errorDisplayField>
    <errorMessage>You cannot select a past date.</errorMessage>
</ValidationRule>
