<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Scheduled_start_date</fullName>
    <active>true</active>
    <description>Scheduled start date Should not be in future.</description>
    <errorConditionFormula>AND(
    NOT(ISBLANK(MUSW__Scheduled_Start_Date__c )),   
    TODAY() &gt; MUSW__Scheduled_Start_Date__c 
)</errorConditionFormula>
    <errorDisplayField>MUSW__Scheduled_Start_Date__c</errorDisplayField>
    <errorMessage>You cannot select a past date.</errorMessage>
</ValidationRule>
